      <div class="form-group">
      <%= f.label 'お名前' %>
      <%= f.text_field :name %>
      </div>
      <div class="form-group">
      <%= f.label 'メールアドレス' %>
      <%= f.text_field :email %>
      </div>
      <div class="form-group">
      <%= f.label '誕生日' %>
      <%= f.date_select :birthday, start_year: 1940, end_year: Time.now.year, use_month_numbers: true, class: "c-select" %>
      </div>
      <div class="form-group">
      <%= f.label '自己紹介' %>
      <%= f.text_area :introduce %>
      </div>
      <div class="form-group">
      <%= f.label 'ホームページ' %>
      <%= f.text_field :homepage %>
      </div>
      <div class="form-group">
      <%= f.label '地域' %>
      <%= f.text_field :location %>
      </div>
      